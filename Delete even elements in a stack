#include <iostream>
#include <stack>

using namespace std;

void showStack(stack<int>&st)
{
    while(!st.empty())
    {
        cout<<st.top()<<endl;
        st.pop();
    }
}

void deleteEvenElements(stack<int>&st, stack<int>&tmpSt)
{
    while(!st.empty())

    {

        if(st.top()%2==1)
        {
            tmpSt.push(st.top());
        }
        st.pop();

    }
    while(!tmpSt.empty())
    {
        st.push(tmpSt.top());
        tmpSt.pop();

    }
}


int main()
{
    stack<int>st;
    st.push(15);
    st.push(20);
    st.push(27);
    st.push(35);
    stack<int> tmpSt;
    deleteEvenElements(st,tmpSt);
    showStack(st);



    return 0;
}
