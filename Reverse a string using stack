#include <iostream>
#include <stack>
#include <string>



int main()
{
    std::stack< char> st;
    std::string str;
    std::string revStr;

    getline(std::cin,str);

    for(int i=0; i<str.size(); i++)
    {
        st.push(str[i]);
    }

    while(!st.empty())
    {
        revStr+=st.top();
        st.pop();
    }
    std::cout<<revStr;





    return 0;
}
